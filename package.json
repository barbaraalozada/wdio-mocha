{
  "name": "wdio-mocha",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "all": "wdio run src/config/wdio.conf.js",
    "all:headless": "wdio run src/config/wdio.conf.js --headless",
    "add:remove:elements": "wdio run src/config/wdio.conf.js --spec 'test/specs/addRemoveElementsTest.js'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "allure:report": "npm run allure:generate && npm run allure:open",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.22.0",
    "@wdio/cli": "^9.22.0",
    "@wdio/local-runner": "^9.22.0",
    "@wdio/mocha-framework": "^9.22.0",
    "@wdio/spec-reporter": "^9.20.0",
    "allure-commandline": "^2.36.0",
    "chai": "^6.2.2",
    "chromedriver": "^143.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "wdio-chromedriver-service": "^8.1.1"
  }
}
